{
    "name": "dealroadshow/keda-bundle",
    "description": "Integrates Keda.sh CRDs with Dealroadshow K8S Framework / bundle",
    "type": "symfony-bundle",
    "require": {
        "php": ">=8.2",
        "dealroadshow/k8s-resources": ">=1.30",
        "dealroadshow/k8s-framework": "^1.0",
        "symfony/dependency-injection": ">=7.1",
        "symfony/http-kernel": ">=7.1",
        "symfony/console": ">=7.1",
        "symfony/config": ">=7.1"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Dealroadshow\\Bundle\\KedaBundle\\": "src/"
        }
    },
    "authors": [
        {
            "name": "petr-buchyn",
            "email": "petr.buchyn@finsight.com"
        }
    ],
    "scripts": {
        "cs": "./tools/php-cs-fixer/vendor/bin/php-cs-fixer fix"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
